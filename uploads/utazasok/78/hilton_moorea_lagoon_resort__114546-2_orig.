<script type="text/javascript">
/*  */
$(document).ready(function(){
		$("#sms_subscribe").ajaxForm({
			dataType: 'json',
			 success: function(responseData, statusText) {
				//alert(responseData+' '+statusText);
				if (responseData.success == 1) {
					$('#sms_verify').show();
					$('#sms_success').hide();
					$('#sms_subscribe').hide();
					$('#subscribe_error').text('');
					$('#verify_error').text('');
				} else {
					$('#subscribe_error').text(responseData.errortxt);
				}
			} 

		});
		$("#sms_verifycode").ajaxForm({
			dataType: 'json',
			 success: function(responseData, statusText) {
				//alert(responseData+' '+statusText);
				if (responseData.success == 1) {
					$('#sms_subscribe').hide();
					$('#sms_verify').hide();
					$('#sms_success').show();
					$('#subscribe_error').text('');
					$('#verify_error').text('');
				} else {
					$('#verify_error').text(responseData.errortxt);
				}
			} 

		});
});
/*  */
</script>
